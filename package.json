{
  "name": "xsdlibrary",
  "version": "1.3.5",
  "description": "Xml Schema Library",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "node index.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sublimedatasys/xsdlib.git"
  },
  "keywords": [
    "xsd",
    "xml",
    "xml schema",
    "xml2xsd",
    "xsd2xml",
    "json2xml",
    "json schema to xsd",
    "json to xml",
    "xml to xsd",
    "xml to xml schema",
    "coverter",
    "xml library",
    "xsd library"
  ],
  "author": "Sublime Data Systems",
  "license": "ISC",
  "dependencies": {
    "fast-xml-parser": "^3.17.3",
    "json-beautify": "^1.1.1",
    "json-to-json-schema": "0.0.4",
    "lodash": "^4.17.15",
    "to-json-schema": "^0.2.5",
    "xml-formatter": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2"
  }
}
